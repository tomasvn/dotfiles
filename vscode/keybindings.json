// Place your key bindings in this file to override the defaults
[
	{ "key": "ctrl+m", "command": "editor.action.toggleMinimap" },
	{ "key": "ctrl+;", "command": "workbench.action.terminal.kill" },
	{ "key": "ctrl+'", "command": "editor.action.indentationToTabs" },
	{ "key": "alt+j", "when": "terminalFocus", "command": "workbench.action.focusActiveEditorGroup" },
	{ "key": "alt+k", "when": "!terminalFocus", "command": "workbench.action.terminal.focus" },
	{ "key": "alt+;", "command": "deleteLeft" },
	{ "key": "alt+o", "command": "-workbench.action.focusSideBar" },
	{ "key": "alt+o", "when": "!sideBarFocus", "command": "workbench.action.focusSideBar" },
	{ "key": "alt+o", "when": "sideBarFocus", "command": "workbench.action.focusActiveEditorGroup" },
	{ "key": "alt+shift+k", "command": "workbench.action.terminal.focusNext" },
	{ "key": "alt+shift+j", "command": "workbench.action.terminal.focusPrevious" },
]